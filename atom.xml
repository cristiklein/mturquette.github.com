---
layout: nil
---
<!-- TODO: update your domain name here if you're using one -->
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>deferred.io</title>
 <link href="http://deferred.io/atom.xml" rel="self"/>
 <link href="http://deferred.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://deferred.io</id>
 <author>
   <name>Michael Turquette</name>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://deferred.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://deferred.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
